{
  "name": "@ustad-web/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf dist && find apps -name 'node_modules' -type d -exec rm -rf {} + && find apps -name 'dist' -type d -exec rm -rf {} +",
    "install:root": "npm install --legacy-peer-deps",
    "install:apps": "cd apps/ustad-web-chatbot && npm install --legacy-peer-deps && cd ../ustad-web-esinav && npm install --legacy-peer-deps && cd ../ustad-web-shell && npm install --legacy-peer-deps && cd ../ustad-web-yesildefter && npm install --legacy-peer-deps",
    "build:verbose": "nx reset && nx run-many --target=build --all --parallel=false --verbose",
    "build:clean": "npm run clean && npm run install:root && npm run install:apps && npm run build:verbose",
    "start:all": "nx run-many --target=serve --projects=ustad-web-esinav,ustad-web-yesildefter,ustad-web-shell --parallel=true",
    "start:chatbot": "cd apps/ustad-web-chatbot && ./start-flask.sh",
    "dev": "concurrently \"npm run start:all\" \"npm run start:chatbot\""
  },
  "private": true,
